<local:RodskaApplication x:Class="RodskaNote.App.RodskaApp"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RodskaNote;assembly=RodskaNote.SDK"
             Startup="Rodska_Startup"
             StartupUri="MainWindow.xaml" Exit="Rodska_Exit"
             >
    <local:RodskaApplication.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Syncfusion.Tools.WPF;Component/FrameWork/Ribbon/themes/MetroStyle.xaml" ></ResourceDictionary>

            </ResourceDictionary.MergedDictionaries>
            <SolidColorBrush x:Key="RodskaTabBackground" Color="#FF55AA7F"/>
            <SolidColorBrush x:Key="RodskaYellowButton" Color="#FFFFE87E"/>
            <SolidColorBrush x:Key="ImageBrush"  Color="Black" />
            
        </ResourceDictionary>
    </local:RodskaApplication.Resources>
    <JumpList.JumpList>
        <JumpList ShowRecentCategory="True" ShowFrequentCategory="True">
            
        </JumpList>
    </JumpList.JumpList>
    
    
</local:RodskaApplication>
